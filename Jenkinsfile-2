pipeline {
  agent none
  stages {
    stage('Back-end') {
      agent {
        docker { image 'maven:3.6.3-jdk-8' }
      }
      steps {
        sh 'mvn --version'
        checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/AradhanaDemo/docker.git']])
        ls
      }
    }
  }
}
